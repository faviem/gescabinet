                    $.ajax({
                        type: $this.attr('method'),
                        url: $this.attr('action'),
                        cache: false,
                        data: $this.serialize(),
                        success: function () {
                            $('#contenumesg').val('');
                            notification();
                            // location.reload();
                        },
                        error: function () {
                        }
                    });
                    event.preventDefault();
                });
                function notification() {
                    $.ajax({
                        type: 'GET',
                        url: "{{ path('notifications') }}",
                        cache: false,
                        success: function (data) {
                            $('#contenudiscute').html(data.contenu);
                            $('.nbrenotepartager').text(data.nbrenote);
                            $('.nbreutilisateurs').text(data.nbreuser);
                            $('.nbreaudience').text(data.nbreaudience);
                            $('.nbretache').text(data.nbretache);
                            $("#contenudiscute .scrollbar1").scrollTop($("#contenudiscute .scrollbar1")[0].scrollHeight);
                        },
                        error: function () {
                            alert('coupé');
                        }});
                }
                notification();
                //nbresms();
                setInterval(notification, 20000);
            });
        </script>

