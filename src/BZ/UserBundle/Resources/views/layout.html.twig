                                                            Nbre d'affaires résolues
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    {% set nbreExprimeetot = 0 %}
                                                    {% set nbreEncourstot = 0 %}
                                                    {% for element in  exercices %}
                                                        {% set nbreExprimee = 0 %}
                                                        {% set nbreEncours = 0 %}
                                                        {% for elt in  affaires %}
                                                            {% if elt.exercice.id==element.id %}
                                                                {% set nbreExprimee = nbreExprimee+1 %}
                                                                {% if elt.estarchivee==0 %}
                                                                    {% set nbreEncours = nbreEncours+1 %}
                                                                {% endif %}
                                                            {% endif %}

                                                        {% endfor %}
                                                        <tr class="gradeA">
                                                            <td class="sorting_1" style="" colspan="1" style="font-size: 11px;" >
                                                                {{ element.annee }} 
                                                            </td>
                                                            <td class="" style="font-size: 12px;text-align: center;" colspan="1">{{  nbreExprimee }} </td>
                                                            <td class="" style="font-size: 12px;text-align: center;" colspan="1">{{  nbreEncours }} </td>
                                                            <td class="" style="font-size: 12px;text-align: center;" colspan="1">{{  nbreExprimee-nbreEncours }} </td>
                                                        </tr>
                                                        {% set nbreExprimeetot = nbreExprimeetot + nbreExprimee %}
                                                        {% set nbreEncourstot = nbreEncourstot + nbreEncours %}
                                                    {% endfor %}
